{
  "name": "playst",
  "private": true,
  "version": "0.0.1",
  "description": "a Playst UI toolkit for building web.",
  "repository": "git@github.com:sihotang/playst.git",
  "author": "Sopar Sihotang <soparsihotang@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "ui",
    "ui-toolkit",
    "playst"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "workspaces": [
    "src/Playst/*"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-links": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "@types/node": "^8.0.49",
    "@types/react": "^15.6.6",
    "@types/react-dom": "^16.0.2",
    "awesome-typescript-loader": "^3.3.0",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "compile": "npm-run-all -p compile:source",
    "compile:source": "tsc -p src/Playst",
    "lint": "npm-run-all -p lint:source",
    "lint:source": "tslint --project src/Playst/tsconfig.json --format stylish",
    "start": "./scripts/start"
  },
  "roots": [
    "<rootDir>/src/Playst",
    "<rootDir>/scripts"
  ]
}
